          <TableColumn fx:id="Prenom" prefWidth="75.0" text="الاسم " />
            <TableColumn fx:id="Type" prefWidth="75.0" text="المهنة " />
            <TableColumn fx:id="Tel" prefWidth="75.0" text="رقم الهاتف " />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TableView>
      <TextField layoutX="170.0" layoutY="33.0" prefHeight="33.0" prefWidth="391.0">
         <font>
            <Font name="System Bold" size="14.0" />
