      <Button layoutX="989.0" layoutY="374.0" mnemonicParsing="false" onAction="#Delete" prefHeight="79.0" prefWidth="119.0" text="حذف المستخدم ">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="989.0" layoutY="510.0" mnemonicParsing="false" onAction="#exit" prefHeight="79.0" prefWidth="119.0" text="خروج">
